#!/usr/bin/env python3
# This file is placed in the Public Domain.


"control"


import sys


from objr import parse
from objd import modules


from objd.main import Cfg, cmnd, scan, wrap


def main():
    "main"
    parse(Cfg, " ".join(sys.argv[1:]))
    scan(modules, Cfg.mod)
    cmnd(Cfg.otxt, print)


if __name__ == "__main__":
    wrap(main)
